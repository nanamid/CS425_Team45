{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "flutter_walkthrough_app",
            "cwd": "${workspaceFolder}/Basic_Flutter_App/flutter_walkthrough_app",
            "request": "launch",
            "type": "dart",
            "program": "${workspaceFolder}/Basic_Flutter_App/flutter_walkthrough_app/lib/main.dart"
        },
        {
            "name": "flutter_walkthrough_app (profile mode)",
            "cwd": "${workspaceFolder}/Basic_Flutter_App/flutter_walkthrough_app",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "${workspaceFolder}/Basic_Flutter_App/flutter_walkthrough_app/lib/main.dart"
        },
        {
            "name": "flutter_walkthrough_app (release mode)",
            "cwd": "${workspaceFolder}/Basic_Flutter_App/flutter_walkthrough_app",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "${workspaceFolder}/Basic_Flutter_App/flutter_walkthrough_app/lib/main.dart"
        }
    ]
}
